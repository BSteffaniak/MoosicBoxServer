"server:debug"                             = "server:debug:cpal"
"server:debug-release"                     = "server:debug-release:cpal"
"server:debug-release:cpal"                = "run --package moosicbox_server --profile debug-release --no-default-features --features=cpal,aac,flac,mp3,opus"
"server:debug-release:pulseaudio-simple"   = "run --package moosicbox_server --profile debug-release --no-default-features --features=pulseaudio-simple,aac,flac,mp3,opus"
"server:debug-release:pulseaudio-standard" = "run --package moosicbox_server --profile debug-release --no-default-features --features=pulseaudio-standard,aac,flac,mp3,opus"
"server:debug:cpal"                        = "run --package moosicbox_server --no-default-features --features=cpal,aac,flac,mp3,opus"
"server:debug:pulseaudio-simple"           = "run --package moosicbox_server --no-default-features --features=pulseaudio-simple,aac,flac,mp3,opus"
"server:debug:pulseaudio-standard"         = "run --package moosicbox_server --no-default-features --features=pulseaudio-standard,aac,flac,mp3,opus"
"server:pulseaudio-simple"                 = "run --package moosicbox_server --release --no-default-features --features=pulseaudio-simple,aac,flac,mp3,opus"
"server:pulseaudio-standard"               = "run --package moosicbox_server --release --no-default-features --features=pulseaudio-standard,aac,flac,mp3,opus"
"server:release"                           = "server:cpal"

tunnel-server                      = "tunnel-server:release"
"tunnel-server:debug"              = "run --package moosicbox_tunnel_server --no-default-features --features=postgres-raw"
"tunnel-server:debug-release"      = "run --package moosicbox_tunnel_server --no-default-features --features=postgres-raw --profile debug-release"
"tunnel-server:debug-release:sqlx" = "run --package moosicbox_tunnel_server --no-default-features --features=postgres-sqlx --profile debug-release"
"tunnel-server:debug:sqlx"         = "run --package moosicbox_tunnel_server --no-default-features --features=postgres-sqlx"
"tunnel-server:release"            = "run --package moosicbox_tunnel_server --no-default-features --features=postgres-raw --release"
"tunnel-server:release:sqlx"       = "run --package moosicbox_tunnel_server --no-default-features --features=postgres-sqlx --release"
